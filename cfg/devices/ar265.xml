<?xml version="1.0" encoding="UTF-8"?>
<Config>
    <Device>
        <!-- Моделируем давление во внутренней камере и относительное положение демпферного поршня -->
        <Order>2</Order>
        <SubStepNum>1</SubStepNum>
        <Solver>euler</Solver>

        <!-- Объём внутренней камеры, м^3 -->
        <Vc>0.001</Vc>
        <!-- Время перемещения демпферного поршня на полный рабочий ход, с -->
        <dempfer_time>60</dempfer_time>
        <!-- Давление в камере, начиная с которого блокируется перемещение демпферного поршня -->
        <p_lock>0.05</p_lock>

        <!-- Ограничение чувствительности при малой или почти полной загруженности -->
        <!-- Минимальное значение коээфициента загруженности -->
        <payload_min>0.1</payload_min>
        <!-- Максимальное значение коээфициента загруженности -->
        <payload_max>0.8</payload_max>

        <!-- Коэффициент уменьшения давления в ТЦ при минимальной загруженности -->
        <reduction_min>0.4</reduction_min>
        <!-- Коэффициент уменьшения давления в ТЦ при максимальной загруженности -->
        <reduction_max>1.0</reduction_max>

        <!-- Коэффициент перетока воздуха -->
        <coeffAirFlow>0.05</coeffAirFlow>

        <!-- Коэффициент чувствительности к разнице текущего и требуемого давления в ТЦ -->
        <A>1.0</A>

    </Device>
</Config>
