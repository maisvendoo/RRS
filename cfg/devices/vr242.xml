<?xml version="1.0" encoding="UTF-8"?>
<Config>
    <Device>
        <Order>1</Order>
        <SubStepNum>1</SubStepNum>
        <Solver>euler</Solver>

        <!-- Режим: 0 - короткосоставный (К), 1 - длинносоставный (Д) -->
        <LongTrainMode>0</LongTrainMode>

        <!-- Ускоритель экстренного торможения: 0 - выключен, 1 - включен -->
        <EmergencyMode>1</EmergencyMode>

        <!-- Объем ускорительной камеры -->
        <Vuk>1e-3</Vuk>

        <!-- Давление срабатывания клапана в камере У2 (разобщение ТМ и ЗР) -->
        <pu2>0.039</pu2>

        <!-- Давление запирания тормозного клапана в длинносоставном режиме -->
        <pbv>0.15</pbv>

        <!-- Давление открытия срывного клапана ускорения экстренного торможения -->
        <psv>0.025</psv>

        <!-- Давление открытия широкого канала в ускорительную камеру (сверхзарядное) -->
        <pwv>0.48</pwv>

        <!-- Зона нечувствительности для главного поршня -->
        <s1_min>-0.015</s1_min>
        <s1_max>0.015</s1_max>

        <!-- Коэффициент перетока между тормозной магистралью и запасным резервуаром -->
        <K1>1.8e-3</K1>

        <!-- Коэффициент перетока между тормозной магистралью и ускорительной камерой -->
        <K2>3.2e-3</K2>

        <!-- Коэффициент перетока между тормозной магистралью и ускорительной камерой-->
        <!-- по широкому каналу при сверхзарядном давлении -->
        <K3>4.0e-2</K3>

        <!-- Коэффициент наполнения ТЦ в режиме Д -->
        <K4>1.5e-3</K4>

        <!-- Коэффициент наполнения через тормозной клапан (режим К и начало режима Д) -->
        <K5>2.0e-3</K5>

        <!-- Коэффициент отпуска в режиме Д -->
        <K6>2.0e-3</K6>

        <!-- Коэффициент отпуска через тормозной клапан (режим К и конец режима Д) -->
        <K7>3.0e-3</K7>

        <!-- Коэффициент дополнительной разрядки ТМ -->
        <K8>2.0e-3</K8>

        <!-- Коэффициент экстренной разрядки ТМ -->
        <K9>1.0e-2</K9>

        <!-- Коэффициент чувствительности к отжатию полого стержня главного поршня от уплотнения на выпускном клапане (наполнение ТЦ) -->
        <A1>200.0</A1>

        <!-- Коэффициент чувствительности к открытию клапана дополнительной разрядки тормозной магистрали главным поршнем -->
        <A2>1000.0</A2>

        <!-- Коэффициент чувствительности к открытию выпускного клапана главного поршня -->
        <A3>1000.0</A3>

        <!-- Коэффициент чувствительности к открытию клапанов поршнем в камере У2 -->
        <A4>500.0</A4>

    </Device>
</Config>