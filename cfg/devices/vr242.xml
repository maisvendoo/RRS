<?xml version="1.0" encoding="UTF-8"?>
<Config>
	<Device>
		
		<Order>6</Order>

		<!-- Условный объем магистральной камеры -->
		<Vmk>3e-3</Vmk>
		<!-- Условный объем камеры У2 -->
		<Vy2>1e-4</Vy2>

		<!-- Темп зарядки магистральной камеры от ТМ -->
		<K1>1e-2</K1>

		<!-- Темп зарядки запасного резервуара -->
		<K2>0.9e-3</K2>		
		
		<!-- Темп наполнения ТЦ в режиме "Д" -->
		<K4>1.86e-3</K4>
		
		<!-- Темп наполнения через тормозной клапан (режим К и начало режима Д) -->
		<K5>1.86e-3</K5>		
		
		<!-- Темп наполнения камеры У2 -->
		<K6>1e-2</K6>		

		<!-- Коэффициент учета расхода из ЗР в ТЦ -->
		<K12>2.5</K12>

		<!-- Темп отпуска в режиме Д -->
		<K13>1.6e-3</K13>
		<!-- Темп отпуска через тормозной клапан (режим К и конец режима Д) -->
		<K15>1.6e-3</K15>

		<A1>1.0</A1>

		<k1>1000.0</k1>
		<k2>1000.0</k2>

		<!-- Давление срабатывания клапана в камере У2 (разобщение ТМ и ЗР) -->
		<Py2>0.037</Py2>

		<!-- Зона нечувствиельности для главного поршня -->
		<s1_min>-0.015</s1_min>
		<s1_max>0.015</s1_max>

		<!-- Давление запирания тормозного клапана -->
		<p_bv>0.15</p_bv>

		<!-- Длинносоставный режим -->
		<LongTrainReg>0</LongTrainReg>ы		

		<!-- Темп дополнительной разрядки ТМ -->
		<K14>1.0</K14>

	</Device>
</Config>